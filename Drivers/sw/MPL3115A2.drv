%-BW_METHOD_BEGIN Init
%ifdef Init
%define! RetVal
%include Common\MPL3115A2Init.inc
uint8_t %'ModuleName'%.%Init()
{
%CODE_BEGIN
	uint8_t data;
	uint8_t val;

	/* Perform a reset */
	data = %'ModuleName'_RST_BIT_MASK;
	if(%@I2C_MPL_Interface@'ModuleName'%.WriteAddress(%'ModuleName'_I2C_ADDR, (uint8_t*)%'ModuleName'_CTRL_REG_1, sizeof(%'ModuleName'_CTRL_REG_1), (uint8_t*)&data, sizeof(data)) != ERR_OK){
		return ERR_FAILED;
	}
	
	/* Wait for the reset to be completed. */
	do{
		%@I2C_MPL_Interface@'ModuleName'%.ReadByteAddress8(%'ModuleName'_I2C_ADDR, %'ModuleName'_CTRL_REG_1, &val);
	} while(val & %'ModuleName'_RST_BIT_MASK == 0x00);
		
	/* Sensor is in Standby mode*/
	
	/* Setting the Time Step */
	data = %TIME_STEP; /* After a reset, CTRL_REG_2 = 0 so we can write directly the desired value*/
	if(%@I2C_MPL_Interface@'ModuleName'%.WriteAddress(%'ModuleName'_I2C_ADDR, (uint8_t*)%'ModuleName'_CTRL_REG_2, sizeof(%'ModuleName'_CTRL_REG_2), (uint8_t*)&data, sizeof(data)) != ERR_OK){
		return ERR_FAILED;
	}
	
	
	/* Set in altimeter mode, set the Output Sample Rate (OSR) */
	data = %ALT_MODE | %OSR | (%OST_MODE ? %'ModuleName'_STANDBY_MODE : %'ModuleName'_ACTIVE_MODE) ; /* After a reset, CTRL_REG_1 = 0 so we can write directly the desired value */
	if(%@I2C_MPL_Interface@'ModuleName'%.WriteAddress(%'ModuleName'_I2C_ADDR, (uint8_t*)%'ModuleName'_CTRL_REG_1, sizeof(%'ModuleName'_CTRL_REG_1), (uint8_t*)&data, sizeof(data)) != ERR_OK){
		return ERR_FAILED;
	}
	
	return ERR_OK;
%CODE_END
}
%endif %-Init
